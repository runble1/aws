{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["../../nextjs-s3-static/src/*"]
    }
  },
  "include": ["../../nextjs-s3-static/next-env.d.ts", "../../nextjs-s3-static/**/*.ts", "../../nextjs-s3-static/**/*.tsx", "../../nextjs-s3-static/.next/types/**/*.ts"],
  "exclude": ["../../nextjs-s3-static/node_modules"]
}
